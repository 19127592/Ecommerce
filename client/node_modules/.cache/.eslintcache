        setInfor(res.data)\r\n                    \r\n                } catch (err) {\r\n                    alert(err.response.data.msg)\r\n                }\r\n            }\r\n            getUser()\r\n            getUsers()\r\n            getOrder(infor)\r\n            \r\n        }\r\n    },[token])\r\n\r\n    const addCart = async(product) => {\r\n        if(!Logged) return alert('Please login to continue')\r\n\r\n        const check = cart.every(item =>{\r\n            return item._id !== product._id\r\n        })\r\n        if(check){\r\n            setCart([...cart,{...product, quantity:1}])\r\n            await axios.patch('/user/addCart',{cart:[...cart,{...product, quantity:1}]},{\r\n                headers: {Authorization: token}\r\n            })\r\n        }else{\r\n            alert(\"This product has been added to cart\")\r\n        }\r\n    }\r\n\r\n    return {\r\n        isLogged: [Logged,isLogged],\r\n        isAdmin: [Admin, isAdmin],\r\n        isSeller: [Seller,isSeller],\r\n        cart: [cart, setCart],\r\n        infor:[infor,setInfor],\r\n        users:[users,setUsers],\r\n        orders:[orders,setOrders],\r\n        addToCart:addCart\r\n    }\r\n}\r\n","C:\\Users\\HP\\Desktop\\Ecommerce\\Ecommerce-Remake\\Ecommerce\\Ecommerce\\client\\src\\components\\mainpage\\Admin\\HandleProduct\\ModifyProduct.js",["190","191","192","193"],"import { Link } from 'react-router-dom'\r\nimport React, { useState, useContext } from \"react\";\r\nimport {GlobalState} from '../../../../GlobalState'\r\nexport default function Login() {\r\n    return (\r\n        <div>\r\n            Create Products Components\r\n        </div>\r\n    )\r\n}","C:\\Users\\HP\\Desktop\\Ecommerce\\Ecommerce-Remake\\Ecommerce\\Ecommerce\\client\\src\\components\\mainpage\\Other\\NotFound\\NotFound.js",[],"C:\\Users\\HP\\Desktop\\Ecommerce\\Ecommerce-Remake\\Ecommerce\\Ecommerce\\client\\src\\components\\mainpage\\HomePage\\ProductBoxUI\\ProductItem.js",["194"],"import React from 're