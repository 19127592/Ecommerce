I.addToCart\r\n    const removeAccent = (str) => {\r\n        str = str.replace(/à|á|ạ|ả|ã|â|ầ|ấ|ậ|ẩ|ẫ|ă|ằ|ắ|ặ|ẳ|ẵ/g, \"a\");\r\n        str = str.replace(/è|é|ẹ|ẻ|ẽ|ê|ề|ế|ệ|ể|ễ/g, \"e\");\r\n        str = str.replace(/ì|í|ị|ỉ|ĩ/g, \"i\");\r\n        str = str.replace(/ò|ó|ọ|ỏ|õ|ô|ồ|ố|ộ|ổ|ỗ|ơ|ờ|ớ|ợ|ở|ỡ/g, \"o\");\r\n        str = str.replace(/ù|ú|ụ|ủ|ũ|ư|ừ|ứ|ự|ử|ữ/g, \"u\");\r\n        str = str.replace(/ỳ|ý|ỵ|ỷ|ỹ/g, \"y\");\r\n        str = str.replace(/đ/g, \"d\");\r\n        str = str.replace(/À|Á|Ạ|Ả|Ã|Â|Ầ|Ấ|Ậ|Ẩ|Ẫ|Ă|Ằ|Ắ|Ặ|Ẳ|Ẵ/g, \"A\");\r\n        str = str.replace(/È|É|Ẹ|Ẻ|Ẽ|Ê|Ề|Ế|Ệ|Ể|Ễ/g, \"E\");\r\n        str = str.replace(/Ì|Í|Ị|Ỉ|Ĩ/g, \"I\");\r\n        str = str.replace(/Ò|Ó|Ọ|Ỏ|Õ|Ô|Ồ|Ố|Ộ|Ổ|Ỗ|Ơ|Ờ|Ớ|Ợ|Ở|Ỡ/g, \"O\");\r\n        str = str.replace(/Ù|Ú|Ụ|Ủ|Ũ|Ư|Ừ|Ứ|Ự|Ử|Ữ/g, \"U\");\r\n        str = str.replace(/Ỳ|Ý|Ỵ|Ỷ|Ỹ/g, \"Y\");\r\n        str = str.replace(/Đ/g, \"D\");\r\n        return str.toLowerCase().trim().split(/\\s+/).join('-');\r\n    }\r\n    return (\r\n        <div className='row-btn'>\r\n                <Link id=\"btn-buy\" to=\"/\" onClick={() => addToCart(product)}>\r\n                    Buy\r\n                </Link>\r\n                <Link id=\"btn-view\" to={'/products/'+removeAccent(product.title)}>\r\n                    View\r\n                </Link>\r\n        </div>\r\n)\r\n}\r\nexport default BtnRender",{"ruleId":"214","severity":1,"message":"215","line":12,"column":27,"nodeType":"216","messageId":"217","endLine":12,"endColumn":37,"fix":"218"},{"ruleId":"219","severity":1,"message":"220","line":1,"column":17,"nodeType":"221","messageId":"222","endLine":1,"endColumn":25},{"ruleId":"219","severity":1,"message":"223","line":27,"column":10,"nodeType":"221","messageId":"222","endLine":27,"endColumn":15},{"ruleId":"219","severity":1,"message":"224","line":27,"column":17,"nodeType":"221","messageId":"222","endLine":27,"endColumn":25},{"ruleId":"219","severity":1,"message":"225","line":34,"column":12,"nodeType":"221","messageId":"222","e