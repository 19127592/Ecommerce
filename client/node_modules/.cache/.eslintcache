        </li>\r\n        : <li>\r\n            <div className=\"usericon\">\r\n            \r\n            {isAdmin ? \r\n              <div className=\"icon\">\r\n                <img src={Admin} alt=\"\" width=\"35\" />\r\n              </div>\r\n            :\r\n              <div className=\"icon\">\r\n                <img src={User} alt=\"\" width=\"35\" />\r\n              </div>\r\n              \r\n            }\r\n            <div className=\"dropdown-menu\">\r\n              <li>\r\n                <Link to=\"/\">\r\n                  {isAdmin\r\n                    ? adminControl2()\r\n                    : userControl()}\r\n                </Link>\r\n              </li>\r\n              <li>{!isAdmin ? <Link to=\"/account\">Your account</Link> : \"\"}</li>\r\n              <li>\r\n                {!isAdmin ? (\r\n                  <Link to=\"/account/history\">History Order</Link>\r\n                ) : (\r\n                  \"\"\r\n                )}\r\n              </li>\r\n              <li>\r\n                <Link to=\"/\" onClick={logOut}>\r\n                  Logout\r\n                </Link>\r\n              </li>\r\n            </div>\r\n          </div>\r\n          </li>\r\n        }\r\n          <li>\r\n            <div className=\"usericon\">\r\n              <div className=\"icon\">\r\n                  <img src={Notif} alt=\"\" width=\"35\" />\r\n              </div>\r\n              <div className=\"dropdown-menu\">\r\n                <li>\r\n                  Notification Components\r\n                </li>\r\n              </div>\r\n            </div>\r\n          </li>\r\n      </>\r\n    );\r\n  };\r\n\r\n  const userControl = () => {\r\n    return (\r\n      <>\r\n        <li>\r\n          <Link to=\"/cart\">Your Cart</Link>\r\n        </li>\r\n      </>\r\n    );\r\n  };\r\n\r\n\r\n\r\n  /*create product nam trong product management */\r\n\r\n  const adminControl = () => {\r\n    return (\r\n      <>\r\n        <li>\r\n          <Link to=\"/user-management\">Data management</Link>\r\n        </li>\r\n      </>\r\n    );\r\n  };\r\n\r\n  const adminControl2 = () => {\r\n    return (\r\n      <>\r\n        <li>\r\n          <Link to=\"/user-management\">User Management</Link>\r\n        </li>\r\n        <li>\r\n          <Link to=\"/product-management\">Product Management</Link>\r\n        </li>\r\n      </>\r\n    );\r\n  };\r\n\r\n  const Search = (e) => {};\r\n\r\n  return (\r\n    //FE (Thu)\r\n    <header>\r\n      <div className=\"menu\">\r\n        <img src={Menu} alt=\"\" width=\"30\" />\r\n      </div>\r\n      <div className=\"logo\">\r\n        <h1>\r\n          <Link to=\"/\">\r\n            {isAdmin ? \"Admin\" : \"rookieSE\"}\r\n          </Link>\r\n        </h1>\r\n      </div>\r\n      <div class=\"search-box\">\r\n        <form onSubmit={Search} action={'/search/' + modifyQuery(search)}>\r\n          <input\r\n            type=\"text\"\r\n            name=\"\"\r\n            placeholder=\"Search...\"\r\n            className=\"search-field\"\r\n            value={search}\r\n            onChange={(e) => setSearch(e.target.value.toLowerCase())}\r\n          />\r\n        </form>\r\n      </div>\r\n      <ul>\r\n        <li>\r\n          <Link to=\"/\">\r\n            {isAdmin ? adminControl() : \"\"}\r\n          </Link>\r\n        </li>\r\n        {\r\n          loggedControl()\r\n        }\r\n        <li>\r\n          <img src={Close} alt=\"\" width=\"30\" className=\"menu\" />\r\n          <div className=\"icon\">\r\n            <div className=\"cart-icon\">\r\n              <span>{cart.length}</span>\r\n              <Link to=\"/cart\">\r\n                <img src={Cart} alt=\"\" width=\"33\" />\r\n              </Link>\r\n            </div>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n    </header>\r\n  );\r\n}\r\n","C:\\Users\\HP\\Desktop\\Ecommerce\\Ecommerce-Remake\\Ecommerce\\Ecommerce\\client\\src\\GlobalState.js",["127"],"import React, {createContext,useStat